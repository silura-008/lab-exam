//SELECTION SORT 
for (i = 0; i < n - 1; i++) {
        minIndex = i;
        for (j = i + 1; j < n; j++) {
            if (arr[j] < arr[minIndex]) {
                minIndex = j;
            }
        }
        // Swap the found minimum element with the element at index i
        temp = arr[minIndex];
        arr[minIndex] = arr[i];
        arr[i] = temp;
    }


